{
	"name": "astro-compressor",
	"version": "1.1.2",
	"description": "A gzip, brotli and zstd compressor for Astro",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/sondr3/astro-compressor.git"
	},
	"author": "Sondre Nilsen <sondre@eons.io> (https://www.eons.io)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/sondr3/astro-compressor/issues"
	},
	"homepage": "https://github.com/sondr3/astro-compressor#readme",
	"keywords": [
		"astro",
		"astro-integration",
		"astro-component",
		"performance",
		"compression"
	],
	"files": [
		"dist/**",
		"CHANGELOG.md",
		"README.md"
	],
	"scripts": {
		"dev": "tsc --watch --incremental",
		"build": "rm -rf dist && tsc",
		"test": "vitest",
		"prepare": "lefthook install",
		"check": "biome check",
		"fix": "biome check --write"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@tsconfig/recommended": "1.0.10",
		"@tsconfig/strictest": "2.0.5",
		"@types/node": "24.1.0",
		"astro": "5.12.4",
		"lefthook": "1.12.2",
		"typescript": "5.8.3",
		"vitest": "3.2.4"
	},
	"engines": {
		"node": ">=22.15.0 <23.0.0 || >=23.8.0"
	},
	"packageManager": "pnpm@10.14.0-0+sha512.2cd47a0cbf5f1d1de7693a88307a0ede5be94e0d3b34853d800ee775efbea0650cb562b77605ec80bc8d925f5cd27c4dfe8bb04d3a0b76090784c664450d32d6"
}
